# Lower cases cyrillic characters

lowercase_cyrillic:
#pragma ALIAS cyrillic_lowercase

# Cyrillic
U+0400 - U+040F  += U+0050
U+0410 - U+042F  += U+0020

Even U+0460 - U+0481 += U+0001
Even U+048A - U+04BF += U+0001
Odd  U+04C1 - U+04CE += U+0001
Even U+04D0 - U+04F5 += U+0001

# Cyrillic Supplementary
Even U+0500 - U+050F += U+0001
