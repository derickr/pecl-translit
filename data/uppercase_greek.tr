# Upper cases greek characters

uppercase_greek:
#pragma ALIAS greek_uppercase

U+03AC              -= U+0026
U+03AD - U+03AF     -= U+0025

U+03B1 - U+03C1     -= U+0020
U+03C2              -= U+001F
U+03C3 - U+03CB     -= U+0020

U+03CC              -= U+0040
U+03CD - U+03CE     -= U+003F

Odd U+03DA - U+03EF -= U+0001

# One way:
U+03D0 = U+0392
U+03D1 = U+0398
U+03D5 = U+03A6
U+03D6 = U+03A0
