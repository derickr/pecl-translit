U+0113,U+0115,U+0117,U+0119,U+011B = U+0065 # e
U+011C,U+011E,U+0120,U+0122 = U+0047 # g
U+011D,U+011F,U+0121,U+0123 = U+0063 # G
U+0124,U+0126               = U+0048 # H
U+0125,U+0127               = U+0068 # h
U+0128,U+012A,U+012C,U+012E,U+0130 = U+0049 # I
U+0129,U+012B,U+012D,U+012F,U+0131 = U+0069 # i
U+0134                      = U+004A # J
U+0135                      = U+006A # j
U+0136                      = U+004B # K
