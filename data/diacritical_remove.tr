U+0174                      = U+0057 # W
U+0175                      = U+0077 # w
U+0176,U+0178               = U+0059 # Y
U+0177                      = U+0079 # y
U+0179,U+017B,U+017D        = U+005A # Z
U+017A,U+017C,U+017E        = U+007A # z

# Latin Extended-B (incomplete)
U+01C4  = U+01F1
U+01C5  = U+01F2
U+01C6  = U+01F3

