# Remove diacriticals from several latin characters

diacritical_remove:
# Latin-1 Supplement
U+00C0 - U+00C5 = U+0041  # A
U+00C7          = U+0043  # C
U+00C8 - U+00CB = U+0045  # E
U+00CC - U+00CF = U+0049  # I
U+00D0          = U+0044  # D
U+00D1          = U+004E  # N
U+00D2 - U+00D6 = U+004F  # O
U+00D8          = U+004F  # O
U+00D9 - U+00DC = U+0055  # U
U+00DD          = U+0059  # Y
U+00E0 - U+00E5 = U+0061  # a
U+00E7          = U+0063  # c
U+00E8 - U+00EB = U+0065  # e
U+00EC - U+00EF = U+0069  # i
U+00F0          = U+0064  # d
U+00F1          = U+006E  # n
U+00F2 - U+00F6 = U+006F  # o
U+00F8          = U+006F  # o
U+00F9 - U+00FC = U+0075  # u
U+00FD          = U+0079  # y
U+00FF          = U+0079  # y

# Latin Extended-A
U+0100,U+0102,U+0104        = U+0041 # A
U+0101,U+0103,U+0105        = U+0061 # a
